#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int sum=1000;
        cout<<sum<<" ";
        
        for(int i=1;i<n;i++){
            int x;
            cin>>x;
            sum+=x;
            cout<<sum<<" ";
        }
        cout<<'\n';
    }

    return 0;
}
